{
    // BEGIN: ENVIL Extension Settings
    "workbench.statusBar.visible": false,
    "workbench.editor.showTabs": "none",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "workbench.activityBar.location": "hidden",
    "window.commandCenter": false,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "workbench.colorCustomizations": {
        "scrollbarSlider.background": "#9aa0",
        "scrollbarSlider.activeBackground": "#f000",
        "scrollbarSlider.hoverBackground": "#ff00",
        "scrollbar.shadow": "#ff00",
        "editorStickyScroll.background": "#f000",
        "editorGutter.addedBackground": "#f000",
        "editorGutter.deletedBackground": "#f000",
        "editorGutter.modifiedBackground": "#f000"
    },
    "editor.stickyScroll.enabled": false,
    "editor.rulers": [],
    "[supercollider]": {
        "editor.quickSuggestions": {
            "other": "on",
            "comments": "off",
            "strings": "off"
        },
        "editor.suggest.showSnippets": true,
        "editor.suggest.showWords": false
    },
    "[javascript]": {
        "editor.quickSuggestions": {
            "other": "off",
            "comments": "off",
            "strings": "off"
        },
        "editor.hover.enabled": false,
        "editor.parameterHints.enabled": false,
        "editor.inlineSuggest.enabled": false,
        "editor.lightbulb.enabled": "off",
        "editor.suggest.localityBonus": false,
        "editor.suggest.showWords": false,
        "editor.suggest.showMethods": false,
        "editor.suggest.showFunctions": false,
        "editor.suggest.showConstructors": false,
        "editor.suggest.showFields": false,
        "editor.suggest.showVariables": false,
        "editor.suggest.showClasses": false,
        "editor.suggest.showStructs": false,
        "editor.suggest.showInterfaces": false,
        "editor.suggest.showModules": false,
        "editor.suggest.showProperties": false,
        "editor.suggest.showEvents": false,
        "editor.suggest.showOperators": false,
        "editor.suggest.showUnits": false,
        "editor.suggest.showValues": false,
        "editor.suggest.showConstants": false,
        "editor.suggest.showEnums": false,
        "editor.suggest.showEnumMembers": false,
        "editor.suggest.showKeywords": false,
        "editor.suggest.showColors": false,
        "editor.suggest.showFiles": false,
        "editor.suggest.showReferences": false,
        "editor.suggest.showCustomcolors": false,
        "editor.suggest.showFolders": false,
        "editor.suggest.showTypeParameters": false,
        "editor.suggest.showSnippets": false
    }
    // END: ENVIL Extension Settings
}